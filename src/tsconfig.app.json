{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "outDir": "../out-tsc/app",
    "baseUrl": "./",
    "module": "es2015",
    "types": []
  },
  "exclude": [
    "test.ts",
    "**/*.spec.ts"
  ],
  "paths": {
    "@app/*": [
      "src/app/*"
    ],
    "@env/*": [
      "src/app/environments/*"
    ],
    "@core/*": [
      "src/app/core/*"
    ],
    "@shared/*": [
      "src/app/shared/*"
    ],
    "@page/*": [
      "src/app/page/*"
    ]
  }
}
